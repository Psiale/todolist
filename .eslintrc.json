{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "no-plusplus": "off",
      "func-names": "off",
      "no-restricted-globals": "off",
      "import/no-mutable-exports": "off",
      "consistent-return": "off"
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }
  